    <groundspeak:cache id="2087469" available="True" archived="False" xmlns:groundspeak="http://www.groundspeak.com/cache/1/0/1">
      <groundspeak:name>DEBB - Festigkeitslehre</groundspeak:name>
      <groundspeak:placed_by>w''(x) = -Mb(x)/EI</groundspeak:placed_by>
      <groundspeak:owner id="2184721">Hastvas_Vascostas</groundspeak:owner>
      <groundspeak:type>Unknown Cache</groundspeak:type>
